<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.screens.orders.listorders.ListOrderController">
    <DatePicker fx:id="datePicker" layoutX="150.0" layoutY="399.0" promptText="Date"/>

    <TextField fx:id="customerTextField" layoutX="150.0" layoutY="434.0" prefHeight="25.0" prefWidth="174.0"
               promptText="Customer"/>

    <TableView fx:id="customersTable" layoutX="52.0" layoutY="44.0" prefHeight="343.0" prefWidth="290.0">
        <columns>
            <TableColumn fx:id="idOrder" prefWidth="33.0" text="Id"/>
            <TableColumn fx:id="orderDate" prefWidth="95.0" text="Order's date"/>
            <TableColumn fx:id="customerId" prefWidth="84.0" text="Customer Id"/>
            <TableColumn fx:id="tableId" prefWidth="77.0" text="Table Id"/>
        </columns>
    </TableView>

    <TableView layoutX="578.0" layoutY="56.0" prefHeight="291.0" prefWidth="290.0">
        <columns>
            <TableColumn prefWidth="33.0" text="Id"/>
            <TableColumn prefWidth="95.0" text="Name"/>
            <TableColumn prefWidth="84.0" text="Price"/>
            <TableColumn prefWidth="77.0" text="Description"/>
        </columns>
    </TableView>
</AnchorPane>

